#!/bin/sh

Red='\033[0;31m'
Green='\033[0;32m'
PURPLE='\033[0;35m'
NC='\033[0m' 

printf "${PURPLE}Lady Berntestel: Unit Tests${NC}\n"
printf "Test 1: "
printf "calcs.λδ\n"
if ../target/lambdadelta<calcs.λδ; then
	printf "${Green}[Test 1: passed]${NC}\n"
else 
	printf "${Red}[Test 1: failed]${NC}\n"
fi 
